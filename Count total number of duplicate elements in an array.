// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int i;
    int count=1;
 printf("Enter the size of the array:");
 scanf("%d",&n);
 int arr[n];
 printf("Enter the elements of the array\n");
 for(i=0;i<n;i++)
 {
     scanf("%d",&arr[i]);
 }
 for(i=0;i<n;i++)
 {
  if(arr[i]==arr[i+1])
  {
      count++;
  }
 }
 printf("Number of duplicate elements:%d",count);
}
