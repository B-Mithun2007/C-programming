// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int i;
 printf("Enter the size of the array:");
 scanf("%d",&n);
 int arr[n];
 printf("Enter the elements of the array\n");
 for(i=0;i<n;i++)
 {
     scanf("%d",&arr[i]);
 }
 printf("Entered elements of the array in reverse order are :");
 for(i=n-1;i>=0;i--)
 {
     printf("%d ",arr[i]);
 }
}
