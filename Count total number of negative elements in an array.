// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int i;
    int count=0;
 printf("Enter the size of the array:");
 scanf("%d",&n);
 int arr[n];
 printf("Enter the elements of the array\n");
 for(i=0;i<n;i++)
 {
     scanf("%d",&arr[i]);
 }
 for(i=0;i<n;i++)
 {
     if(arr[i]<0)
     {
         count++;
     }
 }
 printf("Number of negative elements:%d",count);
}
