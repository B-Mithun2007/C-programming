// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int i;
    int even=0;
    int odd=0;
 printf("Enter the size of the array:");
 scanf("%d",&n);
 int arr[n];
 printf("Enter the elements of the array\n");
 for(i=0;i<n;i++)
 {
     scanf("%d",&arr[i]);
 }
for(i=0;i<n;i++)
{
    if(arr[i]%2==0)
    {
        even++;
    }
    else
    {
        odd++;
    }
}
printf("Number of even elements:%d\n",even);
printf("Number of odd elements:%d\n",odd);
}
