// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
   printf("Try to guess the number\n");
   while(1)
   {
       printf("Enter your number:");
       scanf("%d",&n);
       if(n==7)
       {
           printf("You found the number");
           break;
       }
   }
}
