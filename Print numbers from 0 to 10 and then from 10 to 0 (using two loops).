// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i;
    int j;
    printf("Numbers from 0-10\n");
    for(i=1;i<11;i++)
    {
        printf("%d ",i);
    }
    printf("\n");
    printf("Numbers from 10-0\n");
    for(j=10;j>0;j--)
    {
        printf("%d ",j);
    }
}
