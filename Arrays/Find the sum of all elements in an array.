// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int i;
    int sum=0;
 printf("Enter the size of the array:");
 scanf("%d",&n);
 int arr[n];
 printf("Enter the elements of the array\n");
 for(i=0;i<n;i++)
 {
     scanf("%d",&arr[i]);
     sum=sum+arr[i];
 }
 printf("Sum of the entered elements is: %d",sum);
 
}
