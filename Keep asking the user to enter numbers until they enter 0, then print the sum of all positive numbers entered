// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int sum=0;
    while(1)
    {
    printf("Enter a number:");
    scanf("%d",&n);
    sum=sum+n;
    if(n==0)
    {
        printf("Sum of all entered numbers : %d",sum);
        break;
    }
}
}
